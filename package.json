{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:win": "npx cross-env NODE_ENV=development tsx server/index.ts",
    "dev:clean": "rimraf node_modules/.vite .next coverage && npm run dev:win",
    "setup": "npm install --force && npm install drizzle-zod cross-env tsx rimraf --save",
    "start:local": "npm run setup && npm run dev:win",
    "start:fast": "npx cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "check:quick": "eslint . --ext ts,tsx --max-warnings 5",
    "db:push": "drizzle-kit push",
    "db:setup": "echo Configurando base de datos... && npm run db:push",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:quick": "vitest run --reporter=basic",
    "test:integration": "vitest run server/__tests__/api.integration.test.ts --reporter=verbose",
    "test:integration:watch": "vitest server/__tests__/api.integration.test.ts --watch",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "auto-test": "npm run test:coverage && npm run lint && npm run check",
    "health-check": "node scripts/health-check.js",
    "diagnose": "npm run auto-test && npm run health-check",
    "clean": "rimraf node_modules/.vite .next coverage dist",
    "reset": "npm run clean && npm install --force",
    "super-setup": "node scripts/super-setup.js",
    "super-setup:force": "node scripts/super-setup.js --force",
    "init:fast": "node scripts/super-setup.js --no-server",
    "init:complete": "node scripts/super-setup.js && npm run dev:win",
    "init:safe": "npm install && npm run dev:win",
    "quick-start": "npm install && npm run dev:win",
    "quick-check": "node scripts/quick-health.js",
    "status": "node scripts/quick-health.js",
    "verify-system": "node verificacion-final.js",
    "auto-fix": "node auto-fix-critical.js",
    "repair": "node auto-fix-critical.js --force"
  },
  "dependencies": {
